{
  "expo": {
    "name": "gym",
    "slug": "gym",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.leonmenzies.gym"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "manifest": {
        "application": {
          "android:extractNativeLibs": "true"
        }
      },
      "package": "com.leonmenzies.gym"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "react-native-safe-area-context",
      "react-native-screens"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "cc895fc3-849f-42b4-9a16-b947213f1299"
      },
      "router": {
        "origin": false
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/cc895fc3-849f-42b4-9a16-b947213f1299"
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
